      "source": [
        "def haversine(lon1, lat1, lon2, lat2):\n",
        "    #  Radians conversion\n",
        "    lon1, lat1, lon2, lat2 = map(radians, [lon1, lat1, lon2, lat2])\n",
        "    \n",
        "    # Haversine formula\n",
        "    dlon = lon2 - lon1 \n",
        "    dlat = lat2 - lat1 \n",
        "    a = sin(dlat/2)**2 + cos(lat1) * cos(lat2) * sin(dlon/2)**2\n",
        "    c = 2 * asin(sqrt(a)) \n",
        "    r = 6371000 # Radius of earth in meters\n",
        "    return c * r"
      ]
    },
